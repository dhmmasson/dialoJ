script!= "json="+json
.col.s12.l10.offset-l1.main.z-depth-1#filter2
	.row
		.col.s12
				.row 
					.col.s12 
						div.collection
							each vote in evals								
								-
									var desc = vote.dialogie.replace( /(%s)|(%n)/gi, 
												function( match, p1, p2 ) { 
													if( p1 ) return  dialogie.southPole; 
													if ( p2 ) return dialogie.northPole; 
												} )											
								.collection-item.row.small-Vpadding.valign-wrapper.dialoJ-range( title=desc  )
									-	
										southPoleFont = "accent_"+(7 - vote.voteValue ); 
										northPoleFont = "accent_"+(vote.voteValue - 1 );
										evalId = ["utilité", "", "compréhension", "situation", "redondance", "mal à l'aise"]
									span.col.s3.right-align.valign( class=southPoleFont )= vote.southPole
									span.col.s4.center-align
										.row.no-Vmargin 
											if vote.value != 0 
												input.col.s12.center-align.no-Vmargin.slider_3positions.extreme(  type="range" , min=-1, max=1, value= vote.value )
											else
												input.col.s12.center-align.no-Vmargin.slider_3positions(  type="range" , min=-1, max=1, value= vote.value )
									span.col.s3.valign( class=northPoleFont )= vote.northPole
									span.col.s3
										
		